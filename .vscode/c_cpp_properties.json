{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "/usr/include/**",
                "/usr/include/GL/**",
                "/usr/include/c++/7/**",
                "${workspaceFolder}/**",
                "${workspaceFolder}/include/**"

            ],
            "cStandard": "c11",
            "cppStandard": "c++17",
            "compilerPath": "/usr/bin/g++",
            "intelliSenseMode": "gcc-x64",
            "defines": [
                "_DEBUG",
                "UNICODE"
            ],
            "browse": {
                "limitSymbolsToIncludedHeaders": true,
                "path": [
                    "/usr/include/*",
                    "usr/include/GL/*",
                    "/usr/include/c++/*",
                    "${workspaceFolder}/*",
                    "${workspaceFolder}/include/*"
                ]
            }
        }
    ],
    "version": 4
}